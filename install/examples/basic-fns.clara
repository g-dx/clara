// Entry point
fn main() {
  fn1()
  printf("Fib(25) = %d\n", fib(25))
  printf("5! = %d\n", fact(5))
}

fn fn2() {
    printf("Executing function 2\n")
}

fn fn1() {
    printf("Executing function 1\n")
    fn2()
}

fn fib(n : int) int {
    if n == 0 {
        return 0
    }
    if n == 1 {
        return 1
    }
    return fib(n - 1) + fib(n - 2)
}

fn fact(n: int) int {
    if n == 1 {
        return 1
    }
    return n * fact(n - 1)
}