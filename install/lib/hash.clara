//
// 32-bit fnv-1a implementation
//
// See: http://www.isthe.com/chongo/tech/comp/fnv/index.html#FNV-1a

fn fnv1a(s: string) int {
    hash := 0x811c9dc5
    i := 0
    while i < s.length {
        hash = hash ^ (s[i] & 0xff)
        hash = (hash * 0x01000193) & 0xffffffff
        i = i + 1
    }
    return hash
}