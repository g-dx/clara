fn eval(s: string, sub: string, expected: int) {
    actual := Index(s, sub)
    if actual == expected {
        println("OK")
    } else {
        printf("expected = %d, actual = %d for s = '%s', sub = '%s'\n", expected, actual, s, sub)
    }
}