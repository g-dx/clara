    // Equals
    eqs := stringArray(12, "")
    eqs[0] = "" eqs[1] = ""  // EXPECT: true
    eqs[2] = "a" eqs[3] = "" // EXPECT: false
    eqs[4] = "" eqs[5] = "a" // EXPECT: false
    eqs[6] = "a" eqs[7] = "a" // EXPECT: true
    eqs[8] = "a" eqs[9] = "A" // EXPECT: false
