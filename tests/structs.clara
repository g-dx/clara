fn main() {
    b := byteArray(1)
    b[0] = 127
    i := intArray(1)
    i[0] = 123456789
    a := Arrays(b, i)
    f := Fns(fn() string = "fns.f1" , fn() arrays = a)

    e := Enums(First(a), Second(f))

    s := Structs(a, f, e)

    println(s.a.b[0]) // EXPECT: 127
    println(s.a.i[0]) // EXPECT: 123456789
    println(s.f.f1()) // EXPECT: fns.f1
    println(s.f.f2().i[0]) // EXPECT: 123456789

    match s.e.e1 {
        case First(a):
            println(a.i[0]) // EXPECT: 123456789
        case Second(f):
    }
    match s.e.e2 {
        case First(a):
        case Second(f):
            println(f.f2().b[0]) // EXPECT: 127
    }

    f1 := Fns(fn() string = f.f1(), fn() arrays = f.f2())
    println(f1.f1()) // EXPECT: fns.f1
}

struct structs {
    a : arrays
    f : fns
    e : enums
}

struct arrays {
    b: []byte
    i: []int
}

struct fns {
    f1: fn() string
    f2: fn() arrays
}

struct enums {
    e1: e
    e2: e
}

enum e {
    First(a: arrays)
    Second(f: fns)
}
