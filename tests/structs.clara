fn main() {
    b := byteArray(1)
    b[0] = 127
    i := intArray(1)
    i[0] = 123456789
    a := Arrays(b, i)
    f := Fns(fn() string = "fns.f1" , fn() arrays = a)

    // TODO: local variables z & y are required or else we get heap corruption!!! GC roots & registers!
    z := First(a)
    y := Second(f)
    e := Enums(z, y)

    s := Structs(a, f, e)

    println(s.a.b[0]) // EXPECT: 127
    println(s.a.i[0]) // EXPECT: 123456789
    println(s.f.f1()) // EXPECT: fns.f1
    println(s.f.f2().i[0]) // EXPECT: 123456789

    match s.e.e1 {
        case First(a):
            println(a.i[0]) // EXPECT: 123456789
    }
    match s.e.e2 {
        case Second(f):
            println(f.f2().b[0]) // EXPECT: 127
    }

    // TODO: local variables w & x are required or else we get heap corruption!!! GC roots & registers!
    w := fn() string = f.f1()
    x := fn() arrays = f.f2()
    f1 := Fns(w, x)
    println(f1.f1()) // EXPECT: fns.f1
}

struct structs {
    a : arrays
    f : fns
    e : enums
}

struct arrays {
    b: []byte
    i: []int
}

struct fns {
    f1: fn() string
    f2: fn() arrays
}

struct enums {
    e1: e
    e2: e
}

enum e {
    First(a: arrays)
    Second(f: fns)
}
