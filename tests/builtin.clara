fn main() {
    // Basic types
    printf("i: %d\n", 1234567890)  // EXPECT: i: 1234567890
    printf("i: %#x\n", 0xDEADBEEF) // EXPECT: i: 0xdeadbeef
    printf("s: %s\n", "<string>")  // EXPECT: s: <string>
    printf("b: %d\n", byte(255))   // EXPECT: b: -1
    printf("true: %d\n", true)     // EXPECT: true: 1
    printf("false: %d\n", false)   // EXPECT: false: 0
}

fn byte(i: int) byte = i